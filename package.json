{
  "name": "discord-web",
  "version": "1.0.1",
  "description": "Discord Verification Web",
  "main": "src/server.ts",
  "scripts": {
    "start": "NODE_ENV=production ts-node src/server.ts",
    "start:dev": "NODE_ENV=development yarn nodemon src/server.ts",
    "lint": "yarn eslint .",
    "fix": "yarn eslint --fix .",
    "pretty": "yarn prettier --write .",
    "commit": "lint-staged && yarn git-cz",
    "check": "yarn run ncu -p yarn --color",
    "update": "yarn run ncu -i -p yarn --color"
  },
  "keywords": [
    "tnfAngel"
  ],
  "author": "tnfAngel",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.6.3",
    "dotenv": "^16.0.0",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "express": "^4.17.3",
    "git-cz": "^4.8.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.17",
    "npm-check-updates": "^16.7.2",
    "prettier": "2.6.1",
    "socket.io": "^4.6.0",
    "ts-node": "^10.7.0",
    "tslib": "^2.3.1",
    "typescript": "^4.6.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "node": "^16.14.2"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx}": [
      "prettier --config .prettierrc --write",
      "eslint --fix \"src/**/*.{ts,tsx}\"",
      "eslint \"src/**/*.{ts,tsx}\""
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tnfAngel/discord-verification-web.git"
  },
  "bugs": {
    "url": "https://github.com/tnfAngel/discord-verification-web/issues"
  },
  "homepage": "https://github.com/tnfAngel/discord-verification-web#readme"
}
